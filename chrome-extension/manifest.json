{
  "manifest_version": 2,

  "name": "My Third Extension",
  "description": "今いるページの背景色を変えるよ！",
  "version": "0.1",

  "permissions": [
    "tabs", "http://*/*"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "My Third Extension"
  }
}
